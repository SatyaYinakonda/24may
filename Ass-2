#include<stdio.h>
#include<conio.h>
main()
{
    int a[100],i;
    int num,size,c=0;
    printf("Enter the size of the array:\n");
    scanf("%d",&size);
    printf("The elements of array \n");
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);  
    }
    printf("Enter the number to find its frequency:\n");
    scanf("%d",&num);
    for(i=0;i<size;i++)
    {
        if(a[i]==num)
        c++;
    }
    printf("Frequency is=%d times",c);
   return 0; 
}
